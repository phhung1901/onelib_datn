{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "babenkoivan/elastic-client": "^2.1",
        "babenkoivan/elastic-scout-driver": "^3.1",
        "babenkoivan/elastic-scout-driver-plus": "^4.5",
        "backpack/crud": "4.1.*",
        "backpack/generators": "3.1",
        "backpack/logmanager": "^4.0",
        "backpack/permissionmanager": "^6.0",
        "consoletvs/charts": "6.*",
        "donatello-za/rake-php-plus": "^1.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hocvt/tika-simple": "^1.0",
        "laravel/framework": "^8.0",
        "laravel/sanctum": "^2.11",
        "laravel/scout": "9.4",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.0",
        "laravel/ui": "3.3.2",
        "myclabs/php-enum": "^1.7",
        "patrickschur/language-detection": "^5.1",
        "paypal/paypal-checkout-sdk": "^1.0",
        "php-science/textrank": "^1.2",
        "spatie/laravel-data": "^2.9",
        "voku/portable-utf8": "^6.0",
        "voku/stop-words": "^2.0",
        "vuthaihoc/vi_encoder": "^0.1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.13",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "ThikDev\\": "packages/pdf_parser/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "sed -i -e 's/checkLicenseCodeExists/when/g' vendor/backpack/crud/src/BackpackServiceProvider.php",
            "sed -i -e 's/sendUsageStats/when/g' vendor/backpack/crud/src/BackpackServiceProvider.php"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models --nowrite",
            "@php artisan ide-helper:meta"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
